CPUNAME="Intel Pentium4/Xeon with EM64T"

if use_gcc 3.3. ; then
  CFLAGS="-march=nocona -fPIC -DPIC -mmmx -mfpmath=sse -msse -msse2 -msse3"
elif use_gcc2 ; then
  CFLAGS="-march=i686"
else
  CFLAGS="-march=nocona -fPIC -DPIC -mmmx -mfpmath=sse -msse -msse2 -msse3"
fi

HOST="x86_64-pc-linux-gnu"
FAST="-O3"
SMALL="-Os"

