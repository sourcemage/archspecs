CPUNAME="x86-64, Athlon 64 FX, Opteron and Intel EM64T"

if use_gcc 3.3. ; then
  CFLAGS="-march=x86-64 -fPIC -DPIC -mfpmath=sse -msse -msse2"
elif use_gcc2 ; then
  CFLAGS="-march=i686"
else
  CFLAGS="-march=x86-64 -fPIC -DPIC -mfpmath=sse -msse -msse2"
fi

HOST="x86_64-pc-linux-gnu"
FAST="-O3"
SMALL="-Os"

