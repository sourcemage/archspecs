CPUNAME="Intel Pentium4/Xeon with EM64T, 32bit mode"
if use_gcc 3.3. ; then
  CFLAGS="-march=nocona -m32 -mmmx -mfpmath=sse -msse -msse2 -msse3"
elif use_gcc2 ; then
  CFLAGS="-march=i686"
else
  CFLAGS="-march=nocona -m32 -mmmx -mfpmath=sse -msse -msse2 -msse3"
fi
HOST="i686-pc-linux-gnu"
FAST="-O3"
SMALL="-Os"
