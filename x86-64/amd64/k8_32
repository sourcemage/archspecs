CPUNAME="AMD Athlon 64, Athlon 64 FX and Opteron"
if use_gcc 3.3. ; then
  CFLAGS="-march=athlon-xp -mmmx -mfpmath=sse -msse -msse2 -m3dnow -m32"
elif use_gcc2 ; then
  CFLAGS="-march=i686"
else
  CFLAGS="-march=k8 -mmmx -mfpmath=sse -msse -msse2 -m3dnow -m32"
fi
HOST="i686-pc-linux-gnu"
FAST="-O3"
SMALL="-Os"
